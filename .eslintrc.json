{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-modules":"off",
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-undef": "off",
        "no-unused-vars": "off"
    }
}
